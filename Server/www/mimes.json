		"binary": true
	},
	
	"gif": {
	
		"mime": "image/gif",
		"name": "[GIF Image]",
		"type": "image",
		"parse": "binary",
		"binary": true
	},
	
	"mp3": {
	
		"mime": "audio/mpeg",
		"name": "[MP3 Audio File]",
		"type": "audio",
